<template xmlns="http://ws.apache.org/ns/synapse" name="getColumnHeaders">
	<parameter name="spreadsheetName"/>	
	<parameter name="worksheetName"/>
	<sequence>
		<log level="custom">
			<property name="FLW" value="GoogleSpreadsheet : Get Column Headers"/>			
			<property name="worksheetName" expression="$func:worksheetName"/>
			<property name="spreadsheetName" expression="$func:spreadsheetName"/>		
		</log>
		<class
			name="org.wso2.carbon.connector.googlespreadsheet.GoogleSpreadsheetGetColumnHeaders" />
	</sequence>
</template>	