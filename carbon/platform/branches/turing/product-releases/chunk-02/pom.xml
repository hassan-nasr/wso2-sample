                <module>dependencies</module>
                <module>service-stubs</module>
                <module>components</module>
                <module>platform-integration/test-automation-framework</module>
        	<module>features</module>
                <module>products</module>
		<!--module>../../features/nested-categories/apimgt</module>
		<module>../../features/nested-categories/greg</module>
		<module>../../features/nested-categories/cep</module>
		<module>../../features/repository/4.2.1</module-->
             </modules>
	</profile>
	<profile>
	  <id>builder</id>
	 <activation>
                <property>
                    <name>builder</name>
                    <value>true</value>
                </property>
            </activation>

	     <modules>
		<module>dependencies</module>
                <module>service-stubs</module>
                <module>components</module>
		<module>platform-integration/test-automation-framework</module>
                <module>features</module>
                <module>products</module>
             </modules>
	</profile>
     </profiles>


</project>
