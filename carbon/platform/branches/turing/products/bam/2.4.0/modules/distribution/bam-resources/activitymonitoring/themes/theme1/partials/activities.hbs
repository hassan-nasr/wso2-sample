<div class="container content-starter">
    <div class="row">
        <div class="col-lg-12">
            <h1>Activities</h1>
        </div>
    </div>
</div>
<div class="container content-section-wrapper">
<div class="row">
    <div class="col-lg-12 content-section">

        <table class="table table-striped">
            <thead>
            <tr>
                <th>ID</th>
                <th>Start</th>
                <th>End</th>
            </tr>
            </thead>
            <tbody>
            {{#each activities}}
            <tr>
            <td><a href="{{url "/activity.jag"}}?id={{this.id}}">{{this.id}}</a></td>
            <td>{{this.startTime}}</td>
            <td>{{this.endTime}}</td>
            </tr>
            {{/each}}

        </tbody>
    </table>


</div>
</div>
        </div>
<script>
$().ready(function () {
    // validate the comment form when it is submitted
    $("#searchForm").validate();
    $('#datetimepicker1').datetimepicker({
        language: 'en',
        pick12HourFormat: true
    });
    $('#datetimepicker2').datetimepicker({
        language: 'en',
        pick12HourFormat: true
    });
});
</script>