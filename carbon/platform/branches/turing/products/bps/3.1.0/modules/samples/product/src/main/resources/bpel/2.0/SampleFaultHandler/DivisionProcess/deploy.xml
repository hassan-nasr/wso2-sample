<?xml version="1.0" encoding="UTF-8"?>
<deploy xmlns="http://www.apache.org/ode/schemas/dd/2007/03" xmlns:division="http://wso2.org/wso2con/2011/sample/division">
  <process name="division:DivisionProcess">
    <active>true</active>
    <retired>false</retired>
    <in-memory>false</in-memory>
    <process-events>
      <enable-event>activityLifecycle</enable-event>
    </process-events>
    <provide partnerLink="client">
      <service name="division:DivisionProcessService" port="DivisionProcessPort"/>
    </provide>
  </process>
</deploy>